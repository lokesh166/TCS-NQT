#include<iostream>
using namespace std;

void getReverse(string &str) {
    int s = 0, e = str.length()-1;
    while(s <= e) {
        swap(str[s], str[e]);
        s++; e--;
    }
}

int main() {
    
    
    
    string str = "lokesh";
    
    getReverse(str);
    
    cout << str << endl;
    // output -> hsekol
    return 0;
    
}
